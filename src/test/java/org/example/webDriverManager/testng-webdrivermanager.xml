<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="SmokeTest" verbose="1" >
    <!--
        Pri spusteni tohoto TestNG souboru se pusti dane tridy ktere obsahuji parameter "browser" s value "chrome"
    -->
    <parameter name="browser" value="edge"></parameter>
    <test name="PageTests" >
        <classes>
            <class name="org.example.webDriverManager.test.HomepageTest"/>
            <class name="org.example.webDriverManager.test.LoginPageTest"/>
        </classes>
    </test>
</suite>

