<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="RegressionTests" verbose="1" >
    <test name="localTest">
        <!--
        pri spusteni tohoto TestNG souboru se pro danou tridu hledaji vsechnt anotace, ktere parametr "regression"
        a dane metody se pusti
         -->
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="org.example.localWebDriver.LocalSetupWithTest"/>
        </classes>
    </test>
</suite>

